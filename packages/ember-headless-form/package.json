{
  "name": "ember-headless-form",
  "version": "1.1.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/CrowdStrike/ember-headless-form.git",
  "license": "MIT",
  "author": "CrowdStrike UX Team",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "lint:prettier": "prettier -c .",
    "lint:prettier:fix": "prettier -w .",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@embroider/addon-shim": "^1.0.0",
    "@embroider/util": "^1.10.0",
    "ember-async-data": "^1.0.3",
    "ember-modifier": "^4.1.0",
    "tracked-built-ins": "^3.1.0"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-source": ">=4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/plugin-syntax-decorators": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@embroider/addon-dev": "^5.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.4.0",
    "@glint/environment-ember-loose": "^1.4.0",
    "@glint/environment-ember-template-imports": "^1.4.0",
    "@glint/template": "^1.4.0",
    "@nullvoxpopuli/eslint-configs": "^3.0.2",
    "@tsconfig/ember": "^2.0.0",
    "@types/ember": "^4.0.0",
    "@types/ember__application": "^4.0.0",
    "@types/ember__array": "^4.0.0",
    "@types/ember__component": "^4.0.0",
    "@types/ember__controller": "^4.0.0",
    "@types/ember__debug": "^4.0.0",
    "@types/ember__engine": "^4.0.0",
    "@types/ember__error": "^4.0.0",
    "@types/ember__helper": "^4.0.1",
    "@types/ember__modifier": "^4.0.3",
    "@types/ember__object": "^4.0.0",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.0",
    "@types/ember__runloop": "^4.0.0",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.0",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__utils": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^9.0.0",
    "ember-source": "^5.0.0",
    "ember-template-imports": "^3.4.1",
    "ember-template-lint": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^11.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.3",
    "rollup": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.0",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.75.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/headless-form.js": "./dist/_app_/components/headless-form.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.js",
    "./-private/*": null
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
